<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.comoyo.mapper.UserMapper">

    <select id="gerUserById" resultType="com.comoyo.pojo.User">
        select u.*
        from user u
        where id = #{id};
    </select>
</mapper>